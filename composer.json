{
  "name": "raymauge/rokuwsphp",
  "description": "PHP Library for handling communication with Roku Web Services API",
  "keywords" : ["roku", "web", "services", "api"],
  "homepage" : "https://github.com/rmauge/RokuPHPWS",
  "license": "MIT",
  "authors": [
    {
      "name": "Raymond Mauge"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.4.*"
  },
  "autoload": {
    "psr-4": {
      "RokuWS\\": "src/RokuWS/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RokuWS\\Tests\\": "tests/RokuWS/Tests/"
    }
  }
}
